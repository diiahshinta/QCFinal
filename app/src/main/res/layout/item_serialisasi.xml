<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_grey"
    android:padding="10dp"
    android:layout_marginBottom="20dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <TextView
        android:id="@+id/serialisasi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/shape_pink_1"
        android:padding="10dp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="msgl.io/6893fb92"/>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_duplicate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/item_duplicate"
        tools:itemCount="2"
        app:layout_constraintTop_toBottomOf="@id/serialisasi"/>
</androidx.constraintlayout.widget.ConstraintLayout>